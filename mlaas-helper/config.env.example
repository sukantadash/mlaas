# MLaaS Helper Environment Configuration
# Copy this file to .env and update with your actual values

# Flask Configuration
FLASK_ENV=development
FLASK_APP=server.py

# Keycloak Configuration
KEYCLOAK_URL=https://your-keycloak-instance.com
KEYCLOAK_REALM=your_realm_name
KEYCLOAK_CLIENT_ID=your_client_id
KEYCLOAK_CLIENT_SECRET=your_client_secret

# 3Scale Configuration
THREESCALE_ADMIN_API_URL=https://your-admin-portal.3scale.net/admin/api/
THREESCALE_ADMIN_API_KEY=your_3scale_admin_api_key

# Optional Configuration
API_PAGE_SIZE=500
API_TIMEOUT=30

# Container Registry (if using private registry)
ARTIFACTORY_URL=https://your-artifactory.com
ARTIFACTORY_USERNAME=your_username
ARTIFACTORY_PASSWORD=your_password
ARTIFACTORY_API_KEY=your_api_key 